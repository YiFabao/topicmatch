<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="offlineMessage" name="so.xunta.websocket.entity.OfflineMessage">
    <id name="id">
      <generator class="native"/>
    </id>
    <property name="topicId" not-null="false"/>
    <property name="accepterId" not-null="false" />
    <property name="count" not-null="false"/>
  </class>
</hibernate-mapping>

