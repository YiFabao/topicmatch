<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="weibouser_info" name="so.xunta.entity.WeiboUserInfo">
    <id name="id">
      <generator class="native"/>
    </id>
    <property name="nickname" not-null="false"/>
    <property name="gender" not-null="false"/>
    <property name="location" not-null="false"/>
    <property name="description" not-null="false" />
    <property name="verified_reason" not-null="false" />
    <property name="tags" not-null="false"/>
    <property name="weibo_uid" not-null="false"/>
  </class>
</hibernate-mapping>

